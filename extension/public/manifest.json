
{
  "name": "Eagle Eye",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "images/get_started128.png" 
    }
  },
  "icons": {
    "128": "images/get_started128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-k76zqLG+HQbG+WBCq8JOzI1RH2d8Rqg9Zeca5UqgjOE='; object-src 'self'"
}